<Window x:Class="DOF5RobotControl_GUI.ManualControlWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DOF5RobotControl_GUI"
        mc:Ignorable="d"
        Title="ManualControlWindow" Height="720" Width="1280"
        FontSize="16">
    <DockPanel Margin="8">
        <TextBlock DockPanel.Dock="Bottom" MinHeight="32">Hello, this is the bottom dock.</TextBlock>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Image Grid.Column="0" x:Name="FrameImage"></Image>
            <TextBlock Grid.Column="1" Text="Hello, this is the camera2!" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Grid>
    </DockPanel>
</Window>
